{
    "name": "decodelabs/veneer",
    "description": "Automated static facades",
    "type": "library",
    "keywords": ["library", "tools", "facade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:decodelabs/phpstan-decodelabs.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "decodelabs/glitch": "<2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.16",
        "decodelabs/phpstan-decodelabs": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Veneer\\": "src/Veneer"
        },
        "files": [
            "src/global.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        }
    }
}
